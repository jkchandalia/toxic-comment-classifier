{
    "manifest_version": 2,
  
    "name": "Toxicity Meter",
    "description": "Identify toxic comments on Reddit.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "images/icons8-chrome-16.png",
     "default_popup": "popup.html"
    },
      "permissions": [
     "activeTab",
     "storage",
     "declarativeContent"
     ]
     
  }